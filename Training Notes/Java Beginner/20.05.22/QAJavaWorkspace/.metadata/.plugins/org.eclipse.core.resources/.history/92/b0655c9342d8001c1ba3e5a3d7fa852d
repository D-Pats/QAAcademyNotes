package com.testtwo.athletes;

public class Athletes {
	
	//Attributes
	private String name;
	private String team;
	private int age;
		
	//Default Constructor
	public Athletes() {
		
	}
	
	//Constructor
	public Athletes(String name, String team, int age) {
		
	}
	
	//Getters and Setters
	
	public getName() {
		return name;
	}
	
	public void setName(String name) {
		this.name = name;
	}
	
	public getTeam() {
		return team;
	}
	
	public void setTeam(String team) {
		this.team = team;
	}
	
	public getAge() {
		return age;
	}
	
	public void setAge(int age) {
		this.age = age;
	}

	
	//Methods
	public String toString() {
		return "Name: " + name + "\nTeam: " + team + "\nAge: " + age;
	}
	
}
